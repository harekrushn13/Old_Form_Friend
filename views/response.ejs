<%- include("partials/header") -%>

        <!-- details div -->
        <div class="details-div">
            <h5>Details</h5>
            <form>
                <table class="details-table">
                    <tr>
                        <td class="labletext">Name</td>
                        <td style="width: 2%;vertical-align: top;"><b>:</b></td>
                        <td class="lable-detail"><%= resdata.name %></td>
                    </tr>
                    <tr>
                        <td class="labletext">Enrollment no.</td>
                        <td style="width: 2%;vertical-align: top;"><b>:</b></td>
                        <td class="lable-detail"><%= resdata.enroll %></td>
                    </tr>
                    <tr>
                        <td class="labletext">Email</td>
                        <td style="width: 2%;vertical-align: top;"><b>:</b></td>
                        <td class="lable-detail"><%= resdata.email %></td>
                    </tr>
                    <tr>
                        <td class="labletext">Form Title</td>
                        <td style="width: 2%;vertical-align: top;"><b>:</b></td>
                        <td class="lable-detail"><%= formtitle %></td>
                    </tr>
                    <tr>
                        <td class="labletext">Form Link</td>
                        <td style="width: 2%;vertical-align: top;"><b>:</b></td>
                        <td class="lable-detail"><a href=<%= formlink %>><%= formlink %></a></td>
                    </tr>
                </table>
            </form>
        </div>


        <!-- response details -->
        <div class="response-div" style="min-height: 500px;">
            <h5>Response</h5>
                <table class="response-table">
                    <% questions.forEach(element => { %>
                        <tr>
                            <td class="labletext-res que"><%= element.title %></td>
                            <td style="width: 2%;vertical-align: top;"><b>:</b></td>
                            <td class="lable-detail-res ans"><%= element.answer %></td>
                        </tr>
                    <% }) %>
                </table>
        </div>




        <!-- footer -->
        <%- include("partials/footer") -%>

        <!-- Add Bootstrap CDN -->
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha2/dist/js/bootstrap.bundle.min.js"
            integrity="sha384-qKXV1j0HvMUeCBQ+QVp7JcfGl760yU08IQ+GpUo5hlbpg51QRiuqHAJz8+BrxE/N"
            crossorigin="anonymous"></script>

    </body>

</html>